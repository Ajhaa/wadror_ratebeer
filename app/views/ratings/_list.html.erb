<table class="table table-hover">
  <thead>
  <tr>
    <th></th>
    <th>Average</th>
  </tr>
  </thead>

  <tbody>
  <% items.each do |i| %>
      <tr>
        <td><%= link_to i.name, i %></td>
        <td><%= round(i.average_rating) %></td>
        <td></td>
      </tr>
  <% end %>
  </tbody>
</table>

